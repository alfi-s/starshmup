{
  "name": "js13k-2021",
  "version": "1.0.0",
  "description": "Submission for the js13k event",
  "private": "true",
  "scripts": {
    "start": "npm run build-dev & serve build",
    "build": "npm run build:clean && npm run build:html && rollup -c",
    "build-dev": "npm run build:clean && npm run build:html && rollup -c -m -w",
    "build:clean": "rm -r build/*",
    "build:html": "cp src/index.html build"
  },
  "author": "Alfi Sumadi",
  "license": "MIT",
  "dependencies": {
    "kontra": "^7.3.1"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "rollup": "^2.56.3",
    "rollup-plugin-filesize": "^9.1.1",
    "serve": "^12.0.0"
  }
}

{
  "name": "js13k-2021",
  "version": "1.0.0",
  "description": "Submission for the js13k event",
  "private": "true",
  "scripts": {
    "start": "npm run build-dev & serve build",
    "build": "npm run clean && npm run html && rollup -c",
    "build-dev": "npm run clean && npm run html-file && rollup -c -m -w",
    "clean": "rm -r build/*",
    "html-file": "html-minifier src/index.html -o build/index.html --collapse-whitespace --minify-css true"
  },
  "author": "Alfi Sumadi",
  "license": "MIT",
  "dependencies": {
    "kontra": "^7.3.1"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "html-minifier": "^4.0.0",
    "rollup": "^2.56.3",
    "rollup-plugin-filesize": "^9.1.1",
    "serve": "^12.0.0"
  }
}
